{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "interface-name": ["never-prefix"],
    "member-access": [false],
    "ordered-imports": [false],
    "no-console": [false],
    "no-var-requires": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
